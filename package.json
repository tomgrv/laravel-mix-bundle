{
  "name": "laravel-mix-bundle",
  "version": "${GitVersion_SemVer}",
  "description": "Zero configuration asset bundling",
  "main": "src/index.js",
  "scripts": {
    "cz-init": "commitizen init cz-conventional-changelog --save-dev --save-exact --force",
    "withproxy": "git config --global --add url.\\\"https://\\\".insteadOf ssh://",
    "test": "echo \"No test specified\" && exit 0",
    "lint": "echo \"No lint specified\" && exit 0",
    "prod": "echo \"No prod specified\" && exit 0"
  },
  "keywords": [
    "laravel",
    "laravel mix",
    "mix",
    "webpack",
    "bundle",
    "bundling"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomgrv/laravel-mix-bundle.git"
  },
  "author": "tomgrv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomgrv/laravel-mix-bundle/issues"
  },
  "homepage": "https://github.com/tomgrv/laravel-mix-bundle#readme",
  "peerDependencies": {
    "commitizen": "^3.0.5"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "laravel-mix": "^5.0.0",
    "postcss-url": "^8.0.0",
    "read-pkg-up": "^7.0.0"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.25",
    "cz-conventional-changelog": "^3.0.2",
    "git-flow-versionbumping": "^1.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
